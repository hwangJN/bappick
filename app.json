{
  "expo": {
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/bappick.png",
    "scheme": "bappick",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/bappick.png",
      "resizeMode": "contain",
      "backgroundColor": "#A7D8F5"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bappick.app"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/bappick.png",
        "backgroundImage": "./assets/images/bappick.png",
        "monochromeImage": "./assets/images/bappick.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.bappick.app"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "128dc86f-ffdd-4fbd-a19a-17c6023ea6a6"
      }
    }
  }
}
