{
  "expo": {
    "name": "밥픽",
    "slug": "what2eat",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/bappick.png",
    "scheme": "bappick",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/bappick-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#A7D8F5"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bappick.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/bappick.png",
        "backgroundImage": "./assets/images/bappick.png",
        "monochromeImage": "./assets/images/bappick.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.bappick.app"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-build-properties",
        { "android": { "useAndroidX": true, "enableJetifier": true } }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "128dc86f-ffdd-4fbd-a19a-17c6023ea6a6"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/128dc86f-ffdd-4fbd-a19a-17c6023ea6a6",
      "checkAutomatically": "ON_LOAD"
    }
  }
}
